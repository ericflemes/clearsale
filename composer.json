{
  "name": "clearsale2/integration",
  "license": "GPL-3.0-only",
  "version": "1.0.0",
  "type": "magento2-module",
  "description": "End-to-end Fraud management solution, helping e-commerce merchants protect their business by carefully sorting through orders to give the most accurate determination of Fraud risk, and then handling each transaction as necessary.",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": "~7.0.0|~7.1.0|~7.2.0|~7.3.0|~7.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "squizlabs/php_codesniffer": "2.7.0",
    "phpmd/phpmd": "@stable",
    "pdepend/pdepend": "2.2.2",
    "sjparkinson/static-review": "~4.1",
    "fabpot/php-cs-fixer": "~1.2",
    "lusitanian/oauth": "~0.3 <=0.7.0",
    "phpunit/phpunit-selenium": ">=1.2",
    "sebastian/phpcpd": "^2.0",
    "satooshi/php-coveralls": "^1.0",
    "phpdocumentor/phpdocumentor": "^2.8",
    "magento-ecg/coding-standard":"dev-master",
    "rregeer/phpunit-coverage-check": "^0.1.3"
  },
  "authors":[
    {
      "name":"Eric Lemes",
      "email":"eric.lemes@clear.sale"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "Clearsale\\Integration\\": ""
    }
  },
  "config":{
    "secure-http":false
  }
}
